generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Produto {
  id          Int      @id @default(autoincrement())
  nome        String
  descricao   String?
  preco       Float
  criadoEm    DateTime @default(now())
}

model Cliente {
  id  Int        @id @default(autoincrement())
  nome  String
  email String @unique
  senha String
  criadoEm DateTime @default(now())
}
